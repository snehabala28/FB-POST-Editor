{
    "name": "Fb-Editor",
    "description": "this is a fb post editor extension!",
    "version": "1.0.0",
    "manifest_version": 3,
    "action": {
    },
    "icons": {
      "128": "images/icon.png"
    },
    "permissions": [
      "clipboardRead"
    ],
    "background": {
    },
    "host_permissions": [
        "*://*.facebook.com/*"
    ],
    "content_scripts": [
      {
        "css":[
          "style.css"
        ],
        "js": [
          "jquery-3.1.0.min.js", "arrive.js","content.js"
        ],
        "matches": [
            "*://*.facebook.com/*"
        ],
        "run_at":"document_end"
      }
    ]
  }